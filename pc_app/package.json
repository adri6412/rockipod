{
  "name": "rockbox-music-manager",
  "version": "1.0.0",
  "description": "PC Music Manager for Rockbox FiiO M3K Custom DB",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adri6412/rockipod.git"
  },
  "build": {
    "appId": "com.rockipod.musicmanager",
    "productName": "Rockbox Music Manager",
    "win": {
      "target": "nsis",
      "icon": "icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "**/*",
      "!**/*.py",
      "!renderer/library.json"
    ]
  },
  "keywords": [],
  "author": "Antigravity",
  "license": "ISC",
  "dependencies": {
    "music-metadata": "^7.14.0"
  },
  "devDependencies": {
    "electron": "^34.0.0",
    "electron-builder": "^24.13.3"
  }
}